<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- filemanager -->
	<backgroundcolor>BackgroundColour</backgroundcolor>
	<defaultcontrol always="true">20</defaultcontrol>
	<controls>
		<!-- Heading -->
		<control type="label">
			<left>90</left>
			<top>84</top>
			<width>auto</width>
			<height>80</height>
			<font>Med48</font>
			<aligny>bottom</aligny>
			<textcolor>ffdadada</textcolor>
			<label>7</label>
			<animation delay="400" effect="fade" end="100" start="0" time="200">WindowOpen</animation>
		</control>
		<!-- Main group -->
		<control type="group">
			<include content="DialogZoomAnimation">
				<param name="OpenCondition" value="true"/>
				<param name="CloseCondition" value="true"/>
			</include>
			<!-- Left group -->
			<control type="group">
				<left>90</left>
				<top>174</top>
				<width>825</width>
				<height>816</height>
				<include content="DialogBack">
					<param name="dialog_back_width" value="825"/>
					<param name="dialog_back_height" value="816"/>
					<param name="dialog_heading_width" value="825"/>
				</include>
				<!-- Root label -->
				<include content="DialogHeading">
					<param name="dialog_heading_label_id" value="101"/>
					<param name="dialog_heading_label_width" value="600"/>
					<param name="dialog_heading_label_haspath" value="true"/>
				</include>
				<include content="DialogHeadingCount">
					<param name="dialog_heading_count_width" value="745"/>
					<param name="dialog_heading_count_container_id" value="20"/>
				</include>
				<!-- Left list -->
				<control id="20" type="fixedlist">
					<top>60</top>
					<width>825</width>
					<height>756</height>
					<onleft>Back</onleft>
					<onright>21</onright>
					<onup>20</onup>
					<ondown>20</ondown>
					<movement>4</movement>
					<scrolltime>0</scrolltime>
					<focusposition>4</focusposition>
					<orientation>vertical</orientation>
					<animation condition="String.IsEqual(Container(20).NumItems,0) | String.IsEqual(Container(20).NumItems,1) | [String.IsEqual(Container(20).NumItems,2) + !Container(20).HasParent]" effect="slide" end="0,-336">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,2) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,3) | [String.IsEqual(Container(20).NumItems,4) + !Container(20).HasParent]" effect="slide" end="0,-252">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,4) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,5) | [String.IsEqual(Container(20).NumItems,6) + !Container(20).HasParent]" effect="slide" end="0,-168">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,6) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,7) | [String.IsEqual(Container(20).NumItems,8) + !Container(20).HasParent]" effect="slide" end="0,-84">Conditional</animation>
					<itemlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_no_focus.png</texture>
						</control>
						<control type="label">
							<left>40</left>
							<width>745</width>
							<height>84</height>
							<font>Light32</font>
							<textcolor>NotFocusedColour</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<right>40</right>
							<width>745</width>
							<height>84</height>
							<align>right</align>
							<font>Light32</font>
							<textcolor>ff717171</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_focus.png</texture>
						</control>
						<control type="group">
							<visible>Control.HasFocus(20)</visible>
							<control type="label">
								<left>40</left>
								<width>745</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>FocusedColour</textcolor>
								<label>$INFO[ListItem.Label]</label>
							</control>
							<control type="label">
								<right>40</right>
								<width>745</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
						<control type="group">
							<visible>!Control.HasFocus(20)</visible>
							<control type="label">
								<left>40</left>
								<width>745</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>ffffffff</textcolor>
								<label>$INFO[ListItem.Label]</label>
							</control>
							<control type="label">
								<right>40</right>
								<width>745</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
					</focusedlayout>
				</control>
				<control type="image">
					<top>60</top>
					<width>825</width>
					<height>84</height>
					<visible>Container(20).HasPrevious</visible>
					<texture border="2" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
				</control>
				<control type="image">
					<top>732</top>
					<width>825</width>
					<height>84</height>
					<visible>Container(20).HasNext</visible>
					<texture border="2" diffuse="settings/mask.png">settings/mask_back.png</texture>
				</control>
			</control>
			<!-- Right group -->
			<control type="group">
				<left>1005</left>
				<top>174</top>
				<width>825</width>
				<height>816</height>
				<include content="DialogBack">
					<param name="dialog_back_width" value="825"/>
					<param name="dialog_back_height" value="816"/>
					<param name="dialog_heading_width" value="825"/>
				</include>
				<!-- Root label -->
				<include content="DialogHeading">
					<param name="dialog_heading_label_id" value="102"/>
					<param name="dialog_heading_label_width" value="600"/>
					<param name="dialog_heading_label_haspath" value="true"/>
				</include>
				<include content="DialogHeadingCount">
					<param name="dialog_heading_count_width" value="745"/>
					<param name="dialog_heading_count_container_id" value="21"/>
				</include>
				<!-- Right list -->
				<control id="21" type="fixedlist">
					<top>60</top>
					<width>825</width>
					<height>756</height>
					<onleft>20</onleft>
					<onright>21</onright>
					<onup>21</onup>
					<ondown>21</ondown>
					<movement>4</movement>
					<scrolltime>0</scrolltime>
					<focusposition>4</focusposition>
					<orientation>vertical</orientation>
					<animation condition="String.IsEqual(Container(21).NumItems,0) | String.IsEqual(Container(21).NumItems,1) | [String.IsEqual(Container(21).NumItems,2) + !Container(21).HasParent]" effect="slide" end="0,-336">Conditional</animation>
					<animation condition="[String.IsEqual(Container(21).NumItems,2) + Container(21).HasParent] | String.IsEqual(Container(21).NumItems,3) | [String.IsEqual(Container(21).NumItems,4) + !Container(21).HasParent]" effect="slide" end="0,-252">Conditional</animation>
					<animation condition="[String.IsEqual(Container(21).NumItems,4) + Container(21).HasParent] | String.IsEqual(Container(21).NumItems,5) | [String.IsEqual(Container(21).NumItems,6) + !Container(21).HasParent]" effect="slide" end="0,-168">Conditional</animation>
					<animation condition="[String.IsEqual(Container(21).NumItems,6) + Container(21).HasParent] | String.IsEqual(Container(21).NumItems,7) | [String.IsEqual(Container(21).NumItems,8) + !Container(21).HasParent]" effect="slide" end="0,-84">Conditional</animation>
					<itemlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_no_focus.png</texture>
						</control>
						<control type="label">
							<left>40</left>
							<width>745</width>
							<height>84</height>
							<font>Light32</font>
							<textcolor>NotFocusedColour</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<right>40</right>
							<width>745</width>
							<height>84</height>
							<align>right</align>
							<font>Light32</font>
							<textcolor>ff717171</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_focus.png</texture>
						</control>
						<control type="group">
							<visible>Control.HasFocus(21)</visible>
							<control type="label">
								<left>40</left>
								<width>745</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>FocusedColour</textcolor>
								<label>$INFO[ListItem.Label]</label>
							</control>
							<control type="label">
								<right>40</right>
								<width>745</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
						<control type="group">
							<visible>!Control.HasFocus(21)</visible>
							<control type="label">
								<left>40</left>
								<width>745</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>ffffffff</textcolor>
								<label>$INFO[ListItem.Label]</label>
							</control>
							<control type="label">
								<right>40</right>
								<width>745</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
					</focusedlayout>
				</control>
				<control type="image">
					<top>60</top>
					<width>825</width>
					<height>84</height>
					<visible>Container(21).HasPrevious</visible>
					<texture border="2" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
				</control>
				<control type="image">
					<top>732</top>
					<width>825</width>
					<height>84</height>
					<visible>Container(21).HasNext</visible>
					<texture border="2" diffuse="settings/mask.png">settings/mask_back.png</texture>
				</control>
			</control>
		</control>
	</controls>
</window>
