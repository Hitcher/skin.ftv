<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- gameosd -->
	<defaultcontrol always="true">100</defaultcontrol>
	<onload>ClearProperty(ShowSettings,home)</onload>
	<coordinates>
		<origin x="0" y="0"/>
	</coordinates>
	<controls>
		<!-- Background -->
		<control type="group">
			<include>DialogBackgroundImage</include>
			<visible>!Window.IsActive(GameSaves) + !Window.IsActive(GameVideoFilter) + !Window.IsActive(GameStretchMode) + !Window.IsActive(GameVideoRotation) + !Window.IsActive(GameControllers) + !Window.IsActive(InGameSaves)</visible>
			<!-- Dialog group -->
			<control type="group">
				<include>DialogAnimation</include>
				<centerleft>50%</centerleft>
				<centertop>50%</centertop>
				<width>558</width>
				<height>564</height>
				<!-- Dialog background image -->
				<include content="DialogBack">
					<param name="dialog_back_width" value="558"/>
					<param name="dialog_back_height" value="564"/>
					<param name="dialog_heading_width" value="558"/>
					<param name="dialog_shadow_1_visible" value="false"/>
				</include>
				<!-- Heading -->
				<control type="label" id="1">
					<left>40</left>
					<top>14</top>
					<height>30</height>
					<width>558</width>
					<aligny>center</aligny>
					<font>Bold30</font>
					<textcolor>ffdadada</textcolor>
					<label>35221</label>
				</control>
				<!-- Grouplist template -->
				<control type="grouplist" id="100">
					<top>60</top>
					<width>100%</width>
					<height>504</height>
					<itemgap>0</itemgap>
					<onup>noop</onup>
					<ondown>noop</ondown>
					<!-- Resume button -->
					<control type="button" id="101">
						<width>100%</width>
						<height>84</height>
						<font>Light32</font>
						<textoffsetx>40</textoffsetx>
						<textcolor>NotFocusedColour</textcolor>
						<focusedcolor>FocusedColour</focusedcolor>
						<texturefocus border="3">settings/button_focus.png</texturefocus>
						<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
						<label>35224</label>
						<label2>31139</label2>
						<onclick>Play</onclick>
						<visible>String.IsEmpty(Window(home).Property(ShowSettings))</visible>
					</control>
					<!-- Saves button -->
					<control type="button" id="102">
						<width>100%</width>
						<height>84</height>
						<font>Light32</font>
						<textoffsetx>40</textoffsetx>
						<textcolor>NotFocusedColour</textcolor>
						<focusedcolor>FocusedColour</focusedcolor>
						<texturefocus border="3">settings/button_focus.png</texturefocus>
						<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
						<label>35249</label>
						<onclick>ActivateWindow(InGameSaves)</onclick>
						<visible>String.IsEmpty(Window(home).Property(ShowSettings))</visible>
					</control>
					<!-- Players button -->
					<control type="button" id="103">
						<width>100%</width>
						<height>84</height>
						<font>Light32</font>
						<textoffsetx>40</textoffsetx>
