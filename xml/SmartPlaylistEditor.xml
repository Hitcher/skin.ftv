						<control type="label">
							<left>80</left>
							<width>980</width>
							<height>168</height>
							<font>Light32</font>
							<textcolor>NotFocusedColour</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</itemlayout>
					<focusedlayout height="168" width="1140">
						<control type="image">
							<width>1140</width>
							<height>168</height>
							<texture border="0,0,0,6">settings/button_focus.png</texture>
						</control>
						<control type="label">
							<left>80</left>
							<width>980</width>
							<height>168</height>
							<font>Light32</font>
							<textcolor>FocusedColour</textcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>Control.HasFocus(10)</visible>
						</control>
						<control type="label">
							<left>80</left>
							<width>980</width>
							<height>168</height>
							<font>Light32</font>
							<scroll>false</scroll>
							<textcolor>ffffffff</textcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>!Control.HasFocus(10)</visible>
						</control>
					</focusedlayout>
				</control>
			</control>
			<!-- Button grouplist -->
			<control type="grouplist" id="9001">
				<right>48</right>
				<bottom>66</bottom>
				<width>704</width>
				<height>840</height>
				<onleft>10</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onback>10</onback>
				<itemgap>20</itemgap>
				<align>right</align>
