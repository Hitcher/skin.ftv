					<top>168</top>
					<width>1140</width>
					<height>1176</height>
					<onup>noop</onup>
					<ondown>noop</ondown>
					<onleft>100</onleft>
					<onright>9001</onright>
					<itemgap>0</itemgap>
					<scrolltime>0</scrolltime>
					<orientation>vertical</orientation>
					<itemlayout height="168" width="1140">
						<control type="image">
							<width>1140</width>
							<height>168</height>
							<texture border="0,0,0,6">settings/button_no_focus.png</texture>
