<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- smartplaylistrule -->
	<defaultcontrol always="true">15</defaultcontrol>
	<controls>
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>2400</width>
			<height>792</height>
			<include>DialogZoomAnimation</include>
			<include content="DialogBack">
				<param name="dialog_back_width" value="2400" />
				<param name="dialog_back_height" value="792" />
				<param name="dialog_heading_width" value="1600" />
				<param name="dialog_shadow_1_left" value="1600" />
				<param name="dialog_shadow_2_top" value="624" />
				<param name="dialog_shadow_2_left" value="1200" />
				<param name="dialog_shadow_2_height" value="168" />
				<param name="dialog_shadow_2_visible" value="true" />
			</include>
			<!-- Heading -->
			<include content="DialogHeading">
				<param name="dialog_heading_label" value="21421" />
			</include>
			<include content="DialogHeading">
				<param name="dialog_heading_label_top" value="168" />
				<param name="dialog_heading_label" value="21422" />
			</include>
			<control type="image">
				<top>284</top>
				<width>1600</width>
				<height>4</height>
				<texture>dialogs/default_line.png</texture>
			</control>
			<!-- Left group -->
			<control type="group" id="100">
				<top>288</top>
				<width>1600</width>
				<height>672</height>
				<!-- Rule Field -->
				<control type="button" id="15">
					<width>1600</width>
					<height>168</height>
					<onleft>noop</onleft>
					<onright>9001</onright>
					<onup>noop</onup>
					<ondown>16</ondown>
					<font>Light32</font>
					<textoffsetx>80</textoffsetx>
					<textcolor>NotFocusedColour</textcolor>
					<focusedcolor>FocusedColour</focusedcolor>
					<texturefocus border="1,1,1,6">settings/button_focus.png</texturefocus>
					<texturenofocus border="1,1,1,6">settings/button_no_focus.png</texturenofocus>
					<aligny>center</aligny>
					<reverse>yes</reverse>
				</control>
				<!-- Rule Field label -->
				<control type="label">
					<left>80</left>
					<width>1440</width>
					<height>168</height>
					<font>Light32</font>
					<textcolor>FocusedColour</textcolor>
					<label>$INFO[Control.GetLabel(15)]</label>
					<visible>Control.HasFocus(15)</visible>
				</control>
				<control type="label">
					<left>80</left>
					<width>1440</width>
					<height>168</height>
					<font>Light32</font>
					<textcolor>NotFocusedColour</textcolor>
					<label>$INFO[Control.GetLabel(15)]</label>
					<visible>!Control.HasFocus(15)</visible>
				</control>
				<!-- Rule operator -->
				<control type="button" id="16">
					<top>168</top>
					<width>1600</width>
					<height>168</height>
					<onleft>noop</onleft>
					<onright>9001</onright>
					<onup>15</onup>
					<ondown>17</ondown>
					<font>Light32</font>
					<textoffsetx>80</textoffsetx>
					<textcolor>NotFocusedColour</textcolor>
					<focusedcolor>FocusedColour</focusedcolor>
					<texturefocus border="1,1,1,6">settings/button_focus.png</texturefocus>
					<texturenofocus border="1,1,1,6">settings/button_no_focus.png</texturenofocus>
