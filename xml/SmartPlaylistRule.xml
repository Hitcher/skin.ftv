					<left>80</left>
					<width>1440</width>
					<height>168</height>
					<font>Light32</font>
					<textcolor>FocusedColour</textcolor>
					<label>$INFO[Control.GetLabel(16)]</label>
					<visible>Control.HasFocus(16)</visible>
				</control>
				<control type="label">
					<top>168</top>
					<left>80</left>
					<width>1440</width>
					<height>168</height>
					<font>Light32</font>
					<textcolor>NotFocusedColour</textcolor>
					<label>$INFO[Control.GetLabel(16)]</label>
					<visible>!Control.HasFocus(16)</visible>
				</control>
				<!-- Value edit -->
				<control type="edit" id="17">
					<top>336</top>
					<width>1200</width>
					<height>168</height>
					<onup>16</onup>
					<onleft>noop</onleft>
					<onright>20</onright>
					<ondown>noop</ondown>
					<font>Light32</font>
					<textoffsetx>80</textoffsetx>
					<textcolor>NotFocusedColour</textcolor>
					<focusedcolor>FocusedColour</focusedcolor>
					<hinttext>...</hinttext>
					<texturefocus border="1,1,1,6">settings/button_focus.png</texturefocus>
					<texturenofocus border="1,1,1,6">settings/button_no_focus.png</texturenofocus>
				</control>
				<!-- Browse Button -->
				<control type="button" id="20">
					<left>1200</left>
					<top>336</top>
					<width>400</width>
					<height>168</height>
					<onleft>17</onleft>
					<onright>9001</onright>
					<onup>16</onup>
					<ondown>noop</ondown>
					<font>Light32</font>
					<textwidth>1476</textwidth>
					<textoffsetx>80</textoffsetx>
					<textcolor>NotFocusedColour</textcolor>
					<focusedcolor>FocusedColour</focusedcolor>
					<texturefocus border="1,1,1,6">settings/button_focus.png</texturefocus>
					<texturenofocus border="1,1,1,6">settings/button_no_focus.png</texturenofocus>
					<label>1024</label>
				</control>
			</control>
			<!-- Button grouplist -->
			<control type="grouplist" id="9001">
				<right>48</right>
				<bottom>66</bottom>
				<width>704</width>
				<height>840</height>
				<onleft>100</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onback>100</onback>
				<itemgap>20</itemgap>
				<align>right</align>
				<!-- Cancel Button -->
				<include content="DialogButton">
					<param name="button_id" value="19" />
					<param name="button_label" value="222" />
					<param name="button_visible" value="Control.IsEnabled(19)" />
				</include>
