<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- script-trakt-ContextMenu.xml -->
	<defaultcontrol always="true">111</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<!-- Background -->
		<include>DialogBackgroundImage</include>
		<!-- Dialog group -->
		<control type="group">
			<include>DialogAnimation</include>
			<centerleft>50%</centerleft>
			<top>672</top>
			<width>1228</width>
			<height>816</height>
			<!-- Dialog background image -->
			<control type="image">
				<width>100%</width>
				<height>100%</height>
				<texture border="4">dialogs/DialogMessageBackground.png</texture>
			</control>
			<!-- List -->
			<control type="list" id="111">
				<left>104</left>
				<top>104</top>
				<width>1020</width>
				<height>608</height>
				<onup>noop</onup>
				<ondown>111</ondown>
				<onleft>111</onleft>
				<onright>noop</onright>
				<scrolltime>200</scrolltime>
				<itemlayout height="152" width="1020">
					<control type="image">
						<width>1020</width>
						<height>152</height>
						<texture border="13,15,13,15">dialogs/DialogButtonNF.png</texture>
					</control>
					<control type="label">
						<top>1</top>
						<width>1020</width>
						<height>152</height>
						<font>MedUpper24</font>
						<textcolor>ffd8d8d8</textcolor>
						<align>center</align>
						<info>ListItem.Label</info>
					</control>
				</itemlayout>
				<focusedlayout height="152" width="1020">
					<control type="image">
						<width>1020</width>
						<height>152</height>
						<texture border="13,15,13,15">dialogs/DialogButtonFO.png</texture>
					</control>
					<control type="label">
						<top>1</top>
						<width>1020</width>
						<height>152</height>
						<font>MedUpper24</font>
						<textcolor>ff000000</textcolor>
						<align>center</align>
						<info>ListItem.Label</info>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>
