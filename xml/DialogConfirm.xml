			</control>
		</control>
		<!-- Background -->
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<include>DialogAnimation</include>
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>1544</width>
			<height>388</height>
			<!-- Background -->
			<control type="image">
				<centerleft>50%</centerleft>
				<centertop>50%</centertop>
				<width>100%</width>
				<height>100%</height>
				<include>MessageBackground</include>
			</control>
			<!-- Main group -->
			<control type="grouplist">
				<centerleft>50%</centerleft>
				<centertop>50%</centertop>
				<width>1752</width>
				<height>1056</height>
				<itemgap>0</itemgap>
				<align>center</align>
				<orientation>vertical</orientation>
				<usecontrolcoords>true</usecontrolcoords>
				<animation easing="inout" effect="fade" end="100" start="0" time="60" tween="sine">WindowOpen</animation>
				<animation easing="inout" effect="fade" end="0" start="100" time="60" tween="sine">WindowClose</animation>
				<!-- Heading -->
				<control type="group">
					<width>1752</width>
					<height>194</height>
					<control type="image">
						<width>1752</width>
						<height>194</height>
						<texture border="4">dialogs/DialogConfirm.png</texture>
					</control>
					<control id="1" type="label">
						<left>100</left>
						<top>82</top>
						<width>1432</width>
						<height>76</height>
						<font>Bold38</font>
						<textcolor>ffdadada</textcolor>
					</control>
				</control>
				<!-- Details -->
				<control type="textbox">
					<left>100</left>
					<width>1552</width>
					<height min="70" max="420">auto</height>
					<font>Light28</font>
					<textcolor>ff9d9d9d</textcolor>
					<autoscroll>False</autoscroll>
					<label>$VAR[ConfirmText]</label>
				</control>
				<!--  Progress -->
				<control id="20" type="progress">
					<left>100</left>
					<top>54</top>
					<width>1552</width>
					<height>24</height>
					<info>System.Progressbar</info>
					<visible>Window.IsVisible(progressdialog)</visible>
				</control>
