			<left>-200</left>
			<width>20</width>
			<height>20</height>
			<itemgap>0</itemgap>
			<align>center</align>
			<orientation>horizontal</orientation>
			<!-- Cancel -->
			<control id="10" type="button">
				<onleft>10</onleft>
				<onright>noop</onright>
				<include>HiddenControl</include>
			</control>
			<!-- Yes -->
			<control id="11" type="button">
				<onleft>10</onleft>
				<onright>noop</onright>
				<include>HiddenControl</include>
			</control>
