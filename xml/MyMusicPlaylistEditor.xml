						<width>280</width>
						<height>140</height>
						<align>left</align>
						<font>Light24</font>
						<autoscroll>false</autoscroll>
						<textcolor>ffbbbbbb</textcolor>
						<label>$INFO[Container(100).ListItem.Artist,[COLOR ffffffff][B]$LOCALIZE[557]:[/B][/COLOR] ,[CR]]$INFO[Container(100).ListItem.Album,[COLOR ffffffff][B]$LOCALIZE[558]:[/B] [/COLOR]]</label>
					</control>
				</control>
				<!-- Middle grouplist -->
				<control type="grouplist" id="9000">
					<left>24</left>
					<bottom>33</bottom>
					<width>312</width>
					<height>248</height>
					<onleft>50</onleft>
					<onright>100</onright>
					<onup>noop</onup>
					<ondown>noop</ondown>
					<itemgap>10</itemgap>
					<align>right</align>
					<orientation>vertical</orientation>
					<!-- Open Button -->
					<include content="DialogButton">
						<param name="button_id" value="6"/>
						<param name="button_label" value="31009"/>
						<param name="button_visible" value="Control.IsEnabled(6)"/>
					</include>
					<!-- Save Button -->
					<include content="DialogButton">
						<param name="button_id" value="7"/>
						<param name="button_label" value="31010"/>
						<param name="button_visible" value="Control.IsEnabled(7)"/>
					</include>
					<!-- Clear Button -->
					<include content="DialogButton">
						<param name="button_id" value="8"/>
						<param name="button_label" value="31011"/>
						<param name="button_visible" value="Control.IsEnabled(8)"/>
					</include>
				</control>
			</control>
			<!-- Right group -->
			<control type="group">
				<left>1180</left>
				<top>174</top>
				<width>650</width>
				<height>816</height>
				<include content="DialogBack">
					<param name="dialog_back_width" value="650"/>
					<param name="dialog_back_height" value="816"/>
					<param name="dialog_heading_width" value="650"/>
				</include>
				<include content="DialogHeading">
					<param name="dialog_heading_label" value="31012"/>
					<param name="dialog_heading_label_width" value="600"/>
				</include>
				<include content="DialogHeadingCount">
					<param name="dialog_heading_count_width" value="570"/>
					<param name="dialog_heading_count_container_id" value="100"/>
				</include>
				<!-- Right list -->
				<control id="100" type="fixedlist">
					<top>60</top>
					<width>650</width>
					<height>756</height>
					<onleft>9000</onleft>
					<onright>noop</onright>
					<onup>100</onup>
					<ondown>100</ondown>
					<movement>4</movement>
