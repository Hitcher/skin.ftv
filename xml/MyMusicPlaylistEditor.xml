					<animation condition="[String.IsEqual(Container(100).NumItems,4) + Container(100).HasParent] | String.IsEqual(Container(100).NumItems,5) | [String.IsEqual(Container(100).NumItems,6) + !Container(100).HasParent]" effect="slide" end="0,-168">Conditional</animation>
					<animation condition="[String.IsEqual(Container(100).NumItems,6) + Container(100).HasParent] | String.IsEqual(Container(100).NumItems,7) | [String.IsEqual(Container(100).NumItems,8) + !Container(100).HasParent]" effect="slide" end="0,-84">Conditional</animation>
					<itemlayout height="84" width="650">
						<control type="image">
							<width>650</width>
							<height>84</height>
							<texture border="3">settings/button_no_focus.png</texture>
						</control>
						<control type="label">
							<left>40</left>
							<width>595</width>
							<height>84</height>
							<font>Light32</font>
							<textcolor>NotFocusedColour</textcolor>
							<label>$INFO[ListItem.Label]  </label>
						</control>
						<control type="label">
							<right>40</right>
							<width>595</width>
							<height>84</height>
							<align>right</align>
							<font>Light32</font>
							<textcolor>ff717171</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="84" width="650">
						<control type="image">
							<width>650</width>
							<height>84</height>
							<texture border="3">settings/button_focus.png</texture>
						</control>
						<control type="group">
							<visible>Control.HasFocus(100)</visible>
							<control type="label">
								<left>40</left>
								<width>595</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>FocusedColour</textcolor>
								<label>$INFO[ListItem.Label]  </label>
							</control>
							<control type="label">
								<right>40</right>
								<width>595</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
						<control type="group">
							<visible>!Control.HasFocus(100)</visible>
							<control type="label">
								<left>40</left>
								<width>595</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>ffffffff</textcolor>
								<label>$INFO[ListItem.Label]  </label>
							</control>
							<control type="label">
								<right>40</right>
								<width>595</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
					</focusedlayout>
				</control>
				<control type="image">
					<top>60</top>
					<width>650</width>
					<height>84</height>
					<visible>Container(100).HasPrevious</visible>
					<texture border="2" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
				</control>
				<control type="image">
					<top>732</top>
					<width>650</width>
					<height>84</height>
					<visible>Container(100).HasNext</visible>
					<texture border="2" diffuse="settings/mask.png">settings/mask_back.png</texture>
