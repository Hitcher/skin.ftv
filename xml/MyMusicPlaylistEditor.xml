<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- musicplaylisteditor -->
	<backgroundcolor>BackgroundColour</backgroundcolor>
	<defaultcontrol always="true">6</defaultcontrol>
	<views>50</views>
	<controls>
		<!-- Heading -->
		<control type="label">
			<left>90</left>
			<top>84</top>
			<width>auto</width>
			<height>80</height>
			<font>Med48</font>
			<aligny>bottom</aligny>
			<textcolor>ffdadada</textcolor>
			<label>10503</label>
			<animation delay="400" effect="fade" end="100" start="0" time="200">WindowOpen</animation>
		</control>
		<!-- Main group -->
		<control type="group">
			<include content="DialogZoomAnimation">
				<param name="OpenCondition" value="true"/>
				<param name="CloseCondition" value="true"/>
			</include>
			<!-- Left group -->
			<control type="group">
				<left>90</left>
				<top>174</top>
				<width>650</width>
				<height>816</height>
				<include content="DialogBack">
					<param name="dialog_back_width" value="650"/>
					<param name="dialog_back_height" value="816"/>
					<param name="dialog_heading_width" value="650"/>
				</include>
				<include content="DialogHeading">
					<param name="dialog_heading_label" value="31007"/>
					<param name="dialog_heading_label_width" value="600"/>
				</include>
				<include content="DialogHeadingCount">
					<param name="dialog_heading_count_width" value="570"/>
					<param name="dialog_heading_count_container_id" value="50"/>
				</include>
				<!-- Left list -->
				<control id="50" type="fixedlist">
					<top>60</top>
					<width>650</width>
					<height>756</height>
					<onleft>Back</onleft>
					<onright>9000</onright>
					<onup>50</onup>
					<ondown>50</ondown>
					<movement>4</movement>
					<scrolltime>0</scrolltime>
					<focusposition>4</focusposition>
					<orientation>vertical</orientation>
					<animation condition="String.IsEqual(Container(50).NumItems,0) | String.IsEqual(Container(50).NumItems,1) | [String.IsEqual(Container(50).NumItems,2) + !Container(50).HasParent]" effect="slide" end="0,-336">Conditional</animation>
					<animation condition="[String.IsEqual(Container(50).NumItems,2) + Container(50).HasParent] | String.IsEqual(Container(50).NumItems,3) | [String.IsEqual(Container(50).NumItems,4) + !Container(50).HasParent]" effect="slide" end="0,-252">Conditional</animation>
					<animation condition="[String.IsEqual(Container(50).NumItems,4) + Container(50).HasParent] | String.IsEqual(Container(50).NumItems,5) | [String.IsEqual(Container(50).NumItems,6) + !Container(50).HasParent]" effect="slide" end="0,-168">Conditional</animation>
					<animation condition="[String.IsEqual(Container(50).NumItems,6) + Container(50).HasParent] | String.IsEqual(Container(50).NumItems,7) | [String.IsEqual(Container(50).NumItems,8) + !Container(50).HasParent]" effect="slide" end="0,-84">Conditional</animation>
					<itemlayout height="84" width="650">
						<control type="image">
							<width>650</width>
							<height>84</height>
							<texture border="3">settings/button_no_focus.png</texture>
						</control>
						<control type="label">
							<left>40</left>
							<width>595</width>
							<height>84</height>
							<font>Light32</font>
							<textcolor>NotFocusedColour</textcolor>
							<label>$INFO[ListItem.Label]  </label>
						</control>
						<control type="label">
							<right>40</right>
							<width>595</width>
							<height>84</height>
							<align>right</align>
							<font>Light32</font>
							<textcolor>ff717171</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="84" width="650">
						<control type="image">
							<width>650</width>
							<height>84</height>
							<texture border="3">settings/button_focus.png</texture>
						</control>
						<control type="group">
							<visible>Control.HasFocus(50)</visible>
							<control type="label">
								<left>40</left>
								<width>595</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>FocusedColour</textcolor>
								<label>$INFO[ListItem.Label]  </label>
							</control>
							<control type="label">
								<right>40</right>
								<width>595</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
						<control type="group">
							<visible>!Control.HasFocus(50)</visible>
							<control type="label">
								<left>40</left>
								<width>595</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>ffffffff</textcolor>
								<label>$INFO[ListItem.Label]  </label>
							</control>
							<control type="label">
								<right>40</right>
								<width>595</width>
								<height>84</height>
								<align>right</align>
								<font>Light32</font>
								<textcolor>ffdadada</textcolor>
								<label>$INFO[ListItem.Label2]</label>
							</control>
						</control>
					</focusedlayout>
				</control>
				<control type="image">
					<top>60</top>
					<width>650</width>
					<height>84</height>
					<visible>Container(50).HasPrevious</visible>
					<texture border="2" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
				</control>
				<control type="image">
					<top>732</top>
					<width>650</width>
					<height>84</height>
					<visible>Container(50).HasNext</visible>
					<texture border="2" diffuse="settings/mask.png">settings/mask_back.png</texture>
				</control>
			</control>
			<!-- Middle group -->
			<control type="group">
				<centerleft>50%</centerleft>
				<top>174</top>
				<width>360</width>
				<height>816</height>
				<include content="DialogBack">
					<param name="dialog_back_width" value="360"/>
					<param name="dialog_back_height" value="816"/>
					<param name="dialog_heading_width" value="360"/>
				</include>
				<include content="DialogHeading">
					<param name="dialog_heading_label" value="31008"/>
					<param name="dialog_heading_label_width" value="320"/>
				</include>
				<control type="group">
					<visible>Control.HasFocus(50)</visible>
					<!-- Image -->
					<control type="image">
						<left>40</left>
						<top>100</top>
						<width>293</width>
						<height>297</height>
						<aspectratio>scale</aspectratio>
						<bordersize>0,0,13,17</bordersize>
						<texture fallback="DefaultAddonMusic.png">$INFO[Container(50).ListItem.Icon]</texture>
						<bordertexture infill="false" border="14,18,14,18">dialogs/shadow.png</bordertexture>
					</control>
					<control type="textbox">
						<left>40</left>
						<top>400</top>
						<width>280</width>
						<height>140</height>
						<align>left</align>
						<font>Light24</font>
						<autoscroll>false</autoscroll>
						<textcolor>ffbbbbbb</textcolor>
						<label>$INFO[Container(50).ListItem.Artist,[COLOR ffffffff][B]$LOCALIZE[557]:[/B][/COLOR] ,[CR]]$INFO[Container(50).ListItem.Album,[COLOR ffffffff][B]$LOCALIZE[558]:[/B] [/COLOR]]</label>
					</control>
				</control>
				<control type="group">
					<visible>Control.HasFocus(100)</visible>
					<!-- Image -->
					<control type="image">
						<left>40</left>
						<top>100</top>
						<width>293</width>
						<height>297</height>
						<aspectratio>scale</aspectratio>
