<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- pictureinfo -->
	<defaultcontrol always="true">5</defaultcontrol>
	<controls>
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<include>DialogZoomAnimation</include>
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>2756</width>
			<height>960</height>
			<include content="DialogBack"/>
			<include content="DialogHeading">
				<param name="dialog_heading_label" value="13406"/>
			</include>
			<!-- List -->
			<control id="5" type="fixedlist">
				<top>120</top>
				<width>1956</width>
				<height>840</height>
				<onleft>noop</onleft>
				<onright>9001</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<movement>2</movement>
				<scrolltime>0</scrolltime>
				<focusposition>2</focusposition>
				<orientation>vertical</orientation>
				<animation condition="String.IsEqual(Container(5).NumItems,0) | String.IsEqual(Container(5).NumItems,1) | [String.IsEqual(Container(5).NumItems,2) + !Container(5).HasParent]" effect="slide" end="0,-336">Conditional</animation>
				<animation condition="[String.IsEqual(Container(5).NumItems,2) + Container(5).HasParent] | String.IsEqual(Container(5).NumItems,3) | [String.IsEqual(Container(5).NumItems,4) + !Container(5).HasParent]" effect="slide" end="0,-168">Conditional</animation>
				<itemlayout height="168" width="1956">
					<control type="image">
						<width>1956</width>
						<height>168</height>
						<texture border="0,0,0,6">settings/button_no_focus.png</texture>
					</control>
					<control type="label">
						<left>80</left>
						<width>1716</width>
						<height>168</height>
						<font>Light32</font>
						<textcolor>NotFocusedColour</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>80</left>
						<width>1716</width>
						<height>168</height>
						<font>Light32</font>
						<align>right</align>
						<textcolor>NotFocusedColour</textcolor>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</itemlayout>
				<focusedlayout height="168" width="1956">
					<control type="image">
						<width>1956</width>
						<height>168</height>
						<texture border="0,0,0,6">settings/button_no_focus.png</texture>
					</control>
					<control type="label">
						<left>80</left>
						<width>1716</width>
						<height>168</height>
						<font>Light32</font>
						<textcolor>FocusedColour</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>80</left>
						<width>1716</width>
						<height>168</height>
						<font>Light32</font>
						<align>right</align>
						<textcolor>FocusedColour</textcolor>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="image">
				<top>120</top>
				<width>1956</width>
				<height>168</height>
				<visible>Container(5).HasPrevious</visible>
				<texture border="4" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
			</control>
			<control type="image">
				<top>792</top>
				<width>1956</width>
				<height>168</height>
				<visible>Container(5).HasNext</visible>
				<texture border="4" diffuse="settings/mask.png">settings/mask_back.png</texture>
			</control>
			<control type="grouplist" id="9001">
				<right>48</right>
				<bottom>66</bottom>
				<width>704</width>
				<height>840</height>
				<onleft>5</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onback>5</onback>
				<itemgap>20</itemgap>
				<align>right</align>
				<!-- OK Button -->
				<include content="DialogButton">
					<param name="button_id" value="1"/>
					<param name="button_label" value="186"/>
					<onclick>Dialog.Close(all,true)</onclick>
				</include>
			</control>
		</control>
	</controls>
</window>
