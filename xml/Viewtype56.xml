<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Viewtype56">
		<control type="group">
			<visible>Control.IsVisible(56)</visible>
			<!-- Square List -->
			<control id="56" type="fixedlist">
				<left>-238</left>
				<top>242</top>
				<width>2200</width>
				<height>576</height>
				<onup>Back</onup>
				<ondown>1001</ondown>
				<onleft>Back</onleft>
				<onright>noop</onright>
				<onright condition="!Window.IsVisible(1103)">SetProperty(BounceRight,1,Home)</onright>
				<preloaditems>2</preloaditems>
				<focusposition>1</focusposition>
				<viewtype label="31161">list</viewtype>
				<orientation>horizontal</orientation>
				<defaultcontrol always="true">0</defaultcontrol>
				<scrolltime easing="inout" tween="sine">300</scrolltime>
				<visible>!String.IsEmpty(Container.PluginName) | [!Container.Content(movies) + !Container.Content(sets) + !Container.Content(tvshows) + !Container.Content(seasons) + !Container.Content(episodes) + !Container.Content(songs)]</visible>
				<include>ListBounceAnimation</include>
				<include content="HomeToWindowSlideAnimation">
					<param name="FocusRowID1_1" value="193,10"/>
					<param name="FocusRowID1_2" value="497,-175"/>
					<param name="FocusRowID2_2" value="497,195"/>
					<param name="FocusRowID1_3" value="497,-246"/>
					<param name="FocusRowID2_3" value="497,10"/>
					<param name="FocusRowID3_3" value="497,168"/>
				</include>
				<itemlayout height="576" width="436">
					<control type="group">
						<animation center="250,288" condition="true" effect="zoom" end="85" start="85">Conditional</animation>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="50">views/shadow_nf.png</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<aspectratio align="center">scale</aspectratio>
							<texture background="true" fallback="views/square_fallback.png">$VAR[SquareImage]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>$INFO[ListItem.VideoResolution,views/resolution/,.png]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayWatched.png</texture>
							<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png) + [!Integer.IsEqual(ListItem.Property(NumEpisodes),0) | String.IsEmpty(ListItem.Property(NumEpisodes))]</visible>
						</control>
						<control type="image">
							<bottom>48</bottom>
							<width>480</width>
							<height>125</height>
							<texture border="2">views/default_overlay.png</texture>
							<visible>!ListItem.IsParentFolder + !String.Contains(ListItem.Icon,Default)</visible>
						</control>
						<control type="label">
							<left>18</left>
							<top>447</top>
							<width>446</width>
							<height>42</height>
							<font>Bold44</font>
							<scroll>false</scroll>
							<textcolor>ffffffff</textcolor>
							<shadowcolor>88000000</shadowcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>!ListItem.IsParentFolder</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture>views/OverlayNF.png</texture>
						</control>
					</control>
				</itemlayout>
				<focusedlayout height="576" width="436">
					<control type="group">
						<animation center="250,288" easing="inout" effect="zoom" end="100" start="85" time="300" tween="sine">Focus</animation>
						<animation center="250,288" condition="Container(56).OnNext | Container(56).OnPrevious" easing="inout" effect="zoom" end="85" start="100" time="300" tween="sine">UnFocus</animation>
						<animation center="250,288" condition="Control.HasFocus(4000)" easing="inout" effect="zoom" end="85" start="100" time="300" tween="sine">Conditional</animation>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="50">views/shadow_fo.png</texture>
							<include content="ContainerMoveFadeAnimation">
								<param name="ListID" value="56"/>
							</include>
						</control>
						<control type="image">
							<left>-48</left>
							<centertop>288</centertop>
							<width>576</width>
							<height>576</height>
							<texture border="50">views/shadow_nf.png</texture>
							<include content="FocusedListIDAnimation">
								<param name="ListID" value="56"/>
							</include>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<aspectratio align="center">scale</aspectratio>
							<texture background="true" fallback="views/square_fallback.png">$VAR[SquareImage]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>$INFO[ListItem.VideoResolution,views/resolution/,.png]</texture>
						</control>
						<control type="image">
							<top>48</top>
							<width>90</width>
							<height>90</height>
							<texture>views/OverlayWatched.png</texture>
							<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png) + [!Integer.IsEqual(ListItem.Property(NumEpisodes),0) | String.IsEmpty(ListItem.Property(NumEpisodes))]</visible>
						</control>
						<control type="image">
							<bottom>48</bottom>
							<width>480</width>
							<height>125</height>
							<texture border="2">views/default_overlay.png</texture>
							<visible>!ListItem.IsParentFolder + !String.Contains(ListItem.Icon,Default)</visible>
						</control>
						<control type="label">
							<left>18</left>
							<top>447</top>
							<width>446</width>
							<height>42</height>
							<font>Bold44</font>
							<scroll>false</scroll>
							<textcolor>ffffffff</textcolor>
							<shadowcolor>88000000</shadowcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>!ListItem.IsParentFolder</visible>
						</control>
						<control type="image">
							<top>48</top>
							<width>480</width>
							<height>480</height>
							<texture>views/OverlayNF.png</texture>
							<include content="FocusedListIDAnimation">
								<param name="ListID" value="56"/>
							</include>
						</control>
					</control>
				</focusedlayout>
			</control>
		</control>
	</include>
</includes>
